#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
#define pb   push_back
void solve ()
{
   ll n;
   cin >> n;
     ll x = 4*n;
   for (int i=0; i<n; i++)
   {
      x-=2;
      cout << x << " ";
       
   }
   cout << endl;
 
}
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
 
       int t;
       cin >> t;
       while (t--)
       {
          solve (); 
       }
    
 
}
